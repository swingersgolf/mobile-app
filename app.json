{
  "expo": {
    "name": "Swingers",
    "slug": "swingers-mobile-app",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.swingersgolfapp.swingersgolf",
      "newArchEnabled": true,
      "splash": {
        "image": "./assets/images/splash.png",
        "backgroundColor": "#F9F9F9",
        "resizeMode": "cover",
        "dark": {
          "image": "./assets/images/splash-dark.png",
          "backgroundColor": "#2E2E2E",
          "resizeMode": "cover"
        }
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#07663b"
      },
      "package": "com.aidanfroggatt.mobileapp",
      "newArchEnabled": true,
      "splash": {
        "image": "./assets/images/splash.png",
        "backgroundColor": "#F9F9F9",
        "resizeMode": "cover",
        "dark": {
          "image": "./assets/images/splash-dark.png",
          "backgroundColor": "#2E2E2E",
          "resizeMode": "cover"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "be4fc09e-310b-455d-8845-6681e920a9b3"
      }
    },
    "owner": "swingersgolf",
    "newArchEnabled": true
  }
}
